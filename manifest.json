{
  "manifest_version": 2,

  "name": "JIRAHelper",
  "description": "This is a extension for quick creating JIRA ticket inside SF Inc.",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "http://*/*", "contextMenus"
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/icon16.png",
      "19": "img/icon19.png",
      "32": "img/icon32.png",
      "38": "img/icon38.png",
      "48": "img/icon48.png",
      "64": "img/icon64.png"
    },
    "default_title": "JIRAHelper"
  }
}
